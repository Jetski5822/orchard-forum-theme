@{
    string returnUrl = !string.IsNullOrWhiteSpace(Request.QueryString["ReturnUrl"]) ? Request.QueryString["ReturnUrl"].Split(',').FirstOrDefault() : "/";
}

<fieldset class="save-button">
    <button class="primaryAction" type="submit" name="submit.Save" value="submit.Save">@T("Save")</button>
    <a href="@returnUrl" class="button close">@T("Cancel")</a>
</fieldset>